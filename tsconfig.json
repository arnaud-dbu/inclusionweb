{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "plugins": [
            {
                "name": "next"
            }
        ],
        "paths": {
            "@/*": [
                "./*"
            ]
        }
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        ".next/types/**/*.ts",
        "components/Input.jsx",
        "components/Input.jsx",
        "app/welcome/page.tsx",
        "app/welcome/page.tsx",
        "app/page.jsx",
        "app/page.jsx",
        "app/api/users/route.ts",
        "app/api/users/route.ts",
        "components/form/DropDownCheckboxes.jsx",
        "components/form/DropDownCheckboxes.jsx"
    ],
    "exclude": [
        "node_modules"
    ]
}